{
    "name": "umumax/laravel-vendor-minify",
    "description": "A vendor minify package, to remove tests and documentation and minifing all php files to save space",
    "keywords": ["laravel", "vendor", "cleanup", "minify"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaroslaw Goszowski",
            "email": "goszowski@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "10.x",
        "illuminate/console": "10.x",
        "illuminate/filesystem": "10.x",
        "symfony/finder": "^6.2"
    },
    "autoload": {
        "psr-0": {
            "Umumax\\VendorMinify": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Umumax\\VendorMinify\\VendorMinifyServiceProvider"
            ]
        }
    }
}
